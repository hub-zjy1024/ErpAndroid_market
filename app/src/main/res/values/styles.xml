<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="myButton">
        <item name="android:background">@drawable/btn_click</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
    </style>
    <style name="caigouedit_items">
        <item name="android:textColor">#a85cff</item>
    </style>
    <style name="Popwindow_animation">
        <item name="android:windowEnterAnimation">@anim/pop_show_translate</item>
        <item name="android:windowExitAnimation">@anim/pop_dismiss_translate</item>
    </style>
</resources>
