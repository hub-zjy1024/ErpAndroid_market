    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.b1b.js.erpandroid_market.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.b1b.js.erpandroid_market.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.b1b.js.erpandroid_market.MenuActivity"/>
        <activity android:name="com.b1b.js.erpandroid_market.CaigouActivity"/>
        <activity android:name="com.b1b.js.erpandroid_market.CaigoudanEditActivity"/>
        <activity android:name="com.b1b.js.erpandroid_market.NahuoActivity">
        </activity>
    </application>

</manifest>